{
  "name": "async-notation",
  "version": "0.1.0",
  "description": "Async notation for callback-styled functions and methods.",
  "author": "Artem Varshavin",
  "license": "MIT",
  "keywords": [
    "async",
    "await",
    "callback"
  ],
  "main": "index.lib.js",
  "scripts": {
    "test": "cross-env NODE_ENV=development node --experimental-modules --experimental-json-modules node_modules/jest/bin/jest.js",
    "build": "cross-env NODE_ENV=production babel src/index.mjs --plugins=transform-object-rest-spread,array-includes,starts-with-ends-with,transform-runtime --presets env --out-file index.lib.js",
    "build-test": "cross-env NODE_ENV=production jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mothgears/async-notation.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "mjs",
      "json"
    ],
    "testMatch": [
      "**/tests/**/*.(js|mjs)"
    ],
    "transform": {
      ".mjs": "jest-esm-transformer"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-remove-import-export": "^1.1.0",
    "babel-plugin-starts-with-ends-with": "^1.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "jest": "^24.9.0",
    "jest-esm-transformer": "^1.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
